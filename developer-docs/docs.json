{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "api": {
    "openapi": "https://platform-backend.getalchemystai.com/api/openapi.json"
  },
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/logo/logo.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/api/v1/context/add/post",
              "api-reference/endpoint/api/v1/context/delete/post",
              "api-reference/endpoint/api/v1/context/search/post",
              "api-reference/endpoint/api/v1/context/traces/get",
              "api-reference/endpoint/api/v1/context/traces/{traceId}/delete/delete",
              "api-reference/endpoint/api/v1/context/view/docs/get",
              "api-reference/endpoint/api/v1/context/view/get",
              "api-reference/endpoint/api/v1/org/context/view/post"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://mintlify.com/community",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://getalchemystai.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/logo-light.png",
    "dark": "/logo/logo.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:anuran@getalchemystai.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://platform.getalchemystai.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/getalchemyst",
      "github": "https://github.com/alchemyst-ai",
      "linkedin": "https://linkedin.com/company/alchemystai"
    }
  }
}